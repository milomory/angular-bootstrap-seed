{
  "name": "angular-seed",
  "version": "0.0.6",
  "scripts": {
    "start": "npm run dev-server",
    "production": "NODE_ENV=production webpack",
    "development": "webpack",
    "dev-server": "webpack-dev-server",
    "dev-server-work": "HOST=$(hostname -I | grep -o '[0-9.]*' | head -1) webpack-dev-server",
    "postinstall": "bower install && typings install && npm run fixDistDebugRef",
    "fixDistDebugRef": "mkdir -p node_modules/debug/dist && cp node_modules/debug/debug.js node_modules/debug/dist/",
    "bower-update": "npm-check-updates --packageManager bower",
    "typings-update": "typings ls | awk '$2 ~ /.+/ {print $2}' | xargs -I {} typings i -SG dt~{}",
    "release": "npm run production",
    "npm-update": "ncu -u",
    "update-deps": "npm run bower-update && npm run typings-update && npm run npm-update"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-3": "^6.11.0",
    "bower-webpack-plugin": "^0.1.9",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.25.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "ng-annotate-loader": "^0.1.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  }
}
